{
  "extends": "eslint-config-google",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {

    }
  },
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "max-len": ["error", 200],
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"],
    "require-jsdoc": "off"
  },
  "env": {
    "node": true,
    "es6": true
  }
}
