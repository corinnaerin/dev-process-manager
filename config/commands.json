{
  "rip-sdk": {
    "command": "ripsaw",
    "args": [
      "sdk"
    ],
    "options": {
      "nvm": "8.11.2"
    }
  },
  "qproxy": {
    "command": "npm",
    "args": [
      "start"
    ],
    "options": {
      "cwd": "/Volumes/Code/tools/qualtrics-dev-proxy",
      "env": {
        "port": 8080,
        "https_port": 8443
      },
      "requiresRoot": true
    }
  },
  "dashboards-local": {
    "command": "ei-change-source.rb",
    "args": [
      "dashboard",
      "local"
    ]
  },
  "dashboards-docker": {
    "command": "ei-change-source.rb",
    "args": [
      "dashboard",
      "docker"
    ]
  },
  "start": {
    "command": "start.rb"
  },
  "status": {
    "command": "get-status.rb"
  },
  "ee-dashboards-assets": {
    "command": "npm",
    "args": [
      "run",
      "watch"
    ],
    "options": {
      "cwd": "/Volumes/Code/ei/ee-dashboards-java"
    }
  },
  "node-version": {
    "command": "node",
    "args": [
      "--version"
    ]
  },
  "npm-version": {
    "command": "npm",
    "args": [
      "--version"
    ]
  },
  "pwd": {
    "command": "pwd"
  }
}
