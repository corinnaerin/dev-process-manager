{
  "rip-sdk": {
    "command": "ripsaw",
    "args": ["sdk"],
    "options": {
      "nvm": true,
      "cwd": "/Volumes/Code/tools/ripsaw-cli",
      "stdio": "inherit",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "version-check": {
    "command": "npm",
    "args": ["start"],
    "options": {
      "nvm": true,
      "cwd": "/Volumes/Code/tools/version-check-cli"
    }
  },
  "qproxy": {
    "command": "npm",
    "args": ["start"],
    "options": {
      "cwd": "/Volumes/Code/tools/qualtrics-dev-proxy",
      "env": {
        "port": 80,
        "https_port": 443
      },
      "requiresRoot": true
    }
  },
  "prserver": {
    "command": "npm",
    "args": ["run", "watch"],
    "options": {
      "cwd": "/Volumes/Code/printed-reports/printed-reports-server"
    }
  },
  "vocalize:watch": {
    "command": "grunt",
    "args": ["watch"],
    "options": {
      "nvm": true,
      "cwd": "/Volumes/Code/vocalize",
      "env": {
        "QUALTRICSHOSTNAME": "alpha.corinnak"
      }
    }
  },
  "vocalize:server": {
    "command": "yarn",
    "args": ["start"],
    "options": {
      "nvm": true,
      "cwd": "/Volumes/Code/vocalize",
      "env": {
        "QUALTRICSHOSTNAME": "alpha.corinnak"
      }
    }
  },
  "ee-dashboards-assets": {
    "command": "npm",
    "args": ["run", "watch"],
    "options": {
      "cwd": "/Volumes/Code/ex/ee-dashboard-java"
    }
  },
  "assets-proxy": {
    "command": "npm",
    "args": ["run", "proxy"],
    "options": {
      "cwd": "/Volumes/Code/ex/ee-dashboard-java"
    }
  },
  "node-version": {
    "command": "node",
    "args": ["--version"]
  },
  "npm-version": {
    "command": "npm",
    "args": ["--version"]
  },
  "pwd": {
    "command": "pwd"
  },
  "printenv": {
    "command": "printenv",
    "options": {
      "nvm": "9"
    }
  }
}
